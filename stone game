#include<bits/stdc++.h>
using namespace std;
int main(){
    int t;
    int coins, alice=0, bob=0, ans[2];
    cin>>t;
    for(int i=0 ; i<t; i++){
        
        cin>>coins;
        ans[i] = coins%4;
        
    }
    for(int i=0; i<t; i++){
       if(ans[i] == 1){
            cout<<"No"<<endl;
            
        }
        else{
            cout<<"Yea"<<endl;
        }
    }
    
    return 0;
}
