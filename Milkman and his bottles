#include<bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    int test[t];
    for(int i=0; i<t; i++){
        cin>>test[i];
    }
    int m=0;
    while(t>m){
       int l = test[m];
       //int a[4]={1, 5, 7, 10};
       int c10=0, c7=0, c5=0, c1=0;
       while(l){
           if(l>=10){
               c10 = l/10;
               l = l%10;
               
           }
           if(l<10 && l>=7){
               c7 = l/7;
               l = l%7;
           }
           if(l<7 && l>=5){
               c5 = l/5;
               l = l%5;
           }
           if(l<5 && l>=1){
               c1 = l/1;
               l = l%1;
           }
           
       }
        cout<<c10+c7+c5+c1<<endl;
        
        m++;
    }
    
    return 0;
}
2
17
65
2
7

