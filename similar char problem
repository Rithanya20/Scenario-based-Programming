#include<bits/stdc++.h>
using namespace std;
int main(){
    int str;
    cin>>str;
    string s;
    cin>>s;
    int t;
    cin>>t;
    int p[t];
    int count=0;
    map <char, int> map;
    for(int i=0; i<t; i++){
        cin>>p[i];
        
        }
    for(int i=0; i<t; i++){
        for(int j=0; j<p[i]; j++){
            if(s[j]==s[p[i]-1]){
                count++;
            }
            
        }
        cout<<count-1<<endl;
        count=0;
        
    }
    
    return 0;
}
